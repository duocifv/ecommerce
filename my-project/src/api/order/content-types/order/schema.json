{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Orders"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "customer_name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "orders"
    },
    "total": {
      "type": "integer"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "shipped",
        "completed"
      ]
    }
  }
}
